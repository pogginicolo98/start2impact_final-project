[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/start2impact_final-project/chainBid
ExecStart=/home/ubuntu/start2impact_final-project/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/gunicorn.sock \
          chainBid.wsgi:application

[Install]
WantedBy=multi-user.target
